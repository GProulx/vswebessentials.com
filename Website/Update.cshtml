@{
	Response.ContentType = "text/plain";
	Response.Clear();

	Version we, vs;

	Version.TryParse(Request.QueryString["we"], out we);
	Version.TryParse(Request.QueryString["vs"], out vs);

	if (we == null || vs == null)
	{
		Response.End();
	}

	string downloadUrl = "http://visualstudiogallery.msdn.microsoft.com/56633663-6799-41d7-9df7-0f2a504ca361";

	if (we < new Version(2, 2))
	{
		Response.Write(downloadUrl);
	}
	//else if (we == new Version(2, 3))// && vs == new Version(12, 0, 30714, 00))
	//{
	//	Response.Write("http://supervisorjoost.com");
	//}

	Response.End();
}